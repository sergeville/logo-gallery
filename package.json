    "test:e2e:debug": "playwright test e2e/flows/ --debug",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "format": "prettier --write",
    "fix-imports": "eslint --fix --rule 'import/order: error'",
    "validate-standards": "ts-node scripts/validate-standards.ts",
    "validate-standards:fix": "ts-node scripts/validate-standards.ts --fix",
    "clean-cache": "node scripts/clean-cache.js",
    "dev:clean": "npm run clean-cache && npm run dev"
